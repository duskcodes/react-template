{
  "name": "react-template",
  "version": "0.0.1",
  "repository": "github:dusktrades/react-template",
  "author": "dusktrades",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "npm run clean:dist && webpack --mode=production --progress --profile --color",
    "clean:dist": "rm -rf ./dist",
    "clean:node_modules": "rm -rf ./node_modules",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint:check": "eslint .",
    "lint:fix": "eslint . --fix",
    "start": "webpack serve --mode=development --progress --profile --color"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "autoprefixer": "^10.2.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-inline-dotenv": "^1.6.0",
    "css-loader": "^5.0.1",
    "eslint": "^7.16.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-sonarjs": "^0.5.0",
    "eslint-plugin-unicorn": "^24.0.0",
    "html-webpack-plugin": "^5.0.0-alpha.15",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "mini-css-extract-plugin": "^1.3.3",
    "postcss": "^8.2.4",
    "postcss-loader": "^4.1.0",
    "prettier": "^2.2.1",
    "sass": "^1.30.0",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.0.3",
    "webpack": "^5.11.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  }
}
